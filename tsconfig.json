{
  "compilerOptions": {
    "target": "es2017",
    "module": "commonjs",
    "noImplicitAny": false,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "alwaysStrict": true,
    "esModuleInterop": true,
    "newLine": "LF",
    "pretty": true,
    "strict": true,
    "removeComments": true,
    "preserveConstEnums": true,
    "sourceMap": false,
    "moduleResolution": "node",
    "outDir": "dist"
  }
}
